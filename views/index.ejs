<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Custom RPC</title>
    <meta charset="utf-8">
    <meta name="author"       content="Tundra Fizz">
    <link rel="shortcut icon" href="/img/icon.jpg">
    <link rel="stylesheet"    href="/css/index.css">
    <script src="js/jquery-2-0-2.js"></script>
    <script src="js/index.js"></script>
  </head>

  <body>
    <div class="root">
      <div class="container-left">
        <table class="game-bar">
          <tbody>
            <% for(var i = 0; i < games.length; i++){ var game = games[i]; %>
              <tr><td class="game" id=<%= game["id"] %>><%= game["name"] %></td></tr>
            <% } %>
          </tbody>
        </table>
      </div>

      <div class="container-right">
        <div class="text-top">
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
          <div>item</div>
        </div>

        <div class="text-bot">
          <div>item</div>
          <div>item</div>
          <div>item</div>
        </div>

        <div class="two-buttons">
          <div class="button submit">Submit</div>
          <div class="button stop">Stop</div>
        </div>
      </div>
    </div>
  </body>
</html>
